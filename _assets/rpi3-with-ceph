# Configuration template file used by rpi23-gen-image.sh
# Debian Stretch using the Arm64 for kernel compilation and Debian distribution.

APT_SERVER="ftp.ch.debian.org"
RPI_MODEL=3
RELEASE=buster
BUILD_KERNEL=true
SET_ARCH=64
KERNEL_ARCH=arm64
RELEASE_ARCH=arm64
CROSS_COMPILE=aarch64-linux-gnu-
QEMU_BINARY=/usr/bin/qemu-aarch64-static
KERNEL_DEFCONFIG=bcmrpi3_defconfig
KERNEL_BIN_IMAGE=Image
KERNEL_IMAGE=kernel8.img
KERNEL_BRANCH=rpi-4.19.y
ENABLE_WIRELESS=false
ENABLE_BLUETOOTH=false
ENABLE_SSHD=true
KERNEL_MENUCONFIG=true
TIMEZONE="Europe/Zurich"
KERNEL_CCACHE=true
